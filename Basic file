SELECT 
    omni_ext.LOAN_KEY,
    omni_ext.LOAN_NO,
    omni_ext.AREA_ID,
    fund.INCORP_CNTRY_CD,
    fund.OPERATIONAL_ACCOUNT,
    fund.PENDING_QTY,
    fund.SETTLED_QTY,
    fund.PENDING_AMT,
    fund.SETTLED_AMT,
    fund.CONTRACT_PRICE,
    fund.MARGIN_PRICE,
    fund.BILLING_PRICE,
    fund.RECLAIM_RT,
    fund.NET_QTY,
    fund.CONT_QTY
FROM 
    IAL_SLB.loan_fund fund
JOIN 
    IAL_SLB.loan_omni_ext omni_ext ON omni_ext.LOAN_ID = fund.LOAN_ID
JOIN 
    IAL_SLB.loan_omni omni ON omni.LOAN_ID = omni_ext.LOAN_ID
WHERE 
    omni_ext.AREA_ID = '02'  -- Ensure AREA_ID is '02'
    AND fund.INCORP_CNTRY_CD = 'AU'  -- Filter by country code
    AND omni_ext.LOAN_NO IN ('002K75', '002K76', '002K77', '002K78', '002K79', '002K80', '002K61', '002K82')
    AND EXISTS (
        -- Ensure matching records are present with the exact same values
        SELECT 1
        FROM IAL_SLB.loan_fund fund2
        JOIN IAL_SLB.loan_omni_ext omni_ext2 ON omni_ext2.LOAN_ID = fund2.LOAN_ID
        WHERE 
            omni_ext2.LOAN_KEY = omni_ext.LOAN_KEY
            AND omni_ext2.LOAN_NO = omni_ext.LOAN_NO
            AND omni_ext2.AREA_ID = omni_ext.AREA_ID
            AND fund2.PENDING_QTY = fund.PENDING_QTY
            AND fund2.SETTLED_QTY = fund.SETTLED_QTY
            AND fund2.PENDING_AMT = fund.PENDING_AMT
            AND fund2.SETTLED_AMT = fund.SETTLED_AMT
            AND fund2.CONTRACT_PRICE = fund.CONTRACT_PRICE
            AND fund2.MARGIN_PRICE = fund.MARGIN_PRICE
            AND fund2.BILLING_PRICE = fund.BILLING_PRICE
            AND fund2.NET_QTY = fund.NET_QTY
            AND fund2.CONT_QTY = fund.CONT_QTY
            AND fund2.INCORP_CNTRY_CD = fund.INCORP_CNTRY_CD
            AND fund2.OPERATIONAL_ACCOUNT = fund.OPERATIONAL_ACCOUNT
    );